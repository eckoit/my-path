{
    "name": "baseline",
    "version": "0.0.1",
    "description": "A baseline garden app",
    "long_description": "Fork this and do stuff to it.",
    "url": "https://github.com/garden20/baseline-garden-app",
    "categories": ["learning"],
    "flattr_user_id" : "eckoit",
    "attachments": ["css", "img", "jam", "js",  "index.html", "application.appcache", "templates"],
    "modules": "couch",
    "load": "couch/ddoc",

    "icons": {
        "16": "img/promo/icon_16.png",
        "48": "img/promo/icon_48.png",
        "96": "img/promo/icon_96.png",
        "128": "img/promo/icon_128.png"
    },
    "promo_images": {
        "small": "img/promo/promo_small.png"
    },
    "screenshots": [
        "img/promo/screenshot1.png",
        "img/promo/screenshot2.png"
    ],
    "less": {
        "compile": [ "css/app.less" ]
    },
    "cache_manifests": [
            "application.appcache"
        ],
    "jam" : {
        "include" : ["js/app", "domReady"]
    },
    "dependencies": {
        "modules": null,
        "attachments": null,
        "properties": null,
        "underscore": null,
        "less-precompiler": "1.3.0-kanso.6",
        "cache-manifest-timestamp": null,
        "git-info" : null,
        "kanso-jam" : null
    }
}